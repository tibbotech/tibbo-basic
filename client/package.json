{
    "name": "tibbo-basic",
    "displayName": "Tibbo Basic",
    "version": "1.1.5",
    "publisher": "Tibbo",
    "description": "Tibbo Basic Language Support",
    "main": "./out/extension",
    "scripts": {
        "prepublish": "tsc",
        "compile": "tsc",
        "build": "tsc",
        "watch": "tsc -w"
    },
    "author": {
        "name": "Jimmy Hu",
        "email": "jimmy.hu@tibbo.com"
    },
    "engines": {
        "vscode": "^1.48.0"
    },
    "license": "MIT",
    "dependencies": {
        "vscode-languageclient": "6.1.4"
    },
    "devDependencies": {
        "@types/node": "^14.14.21",
        "@types/vscode": "^1.48.0",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "eslint": "^7.2.0",
        "ts-loader": "^7.0.5",
        "typescript": "^3.9.3",
        "vsce": "1.74.0"
    },
    "activationEvents": [
        "onLanguage:tibbo-basic"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tibbo-basic",
                "aliases": [
                    "Tibbo Basic"
                ],
                "extensions": [
                    ".tbs",
                    ".tbh",
                    ".tph",
                    ".tpr"
                ],
                "configuration": "./out/tibbo-basic-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tibbo-basic",
                "scopeName": "source.tibbo-basic",
                "path": "./out/tibbo-basic.tmLanguage.json"
            }
        ]
    }
}
