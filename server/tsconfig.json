{
	"compilerOptions": {
		"target": "es2019",
		"lib": ["ES2019"],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"allowJs": true,
		"strict": true,
		"outDir": "out",
		"rootDir": "src",
		"skipLibCheck": true,
		"noImplicitAny": false
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
